@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<form id="form1">
    <div>
        <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px">
            <ul class="nav nav-tabs">
                <li class="active"><a href="#">车辆信息</a></li>
            </ul>
            <div style="padding-top: 20px; margin-right: 30px;">
                <table class="form">
                    <tr>
                        <th class="formTitle">车 牌 号：</th>
                        <td class="formValue">
                            <input id="F_LicensePlate" name="F_LicensePlate" type="text" class="form-control required" placeholder="车 牌 号" />
                        </td>
                        <th class="formTitle">行驶证号：</th>
                        <td class="formValue">
                            <input id="F_DLNumber" name="F_DLNumber" type="text" class="form-control required" placeholder="行驶证号" />
                        </td>
                    </tr>
                    <tr>
                        <th class="formTitle">司机姓名：</th>
                        <td class="formValue">
                            <input id="F_Name" name="F_Name" type="text" class="form-control required" placeholder="司机姓名" />
                        </td>
                        <th class="formTitle">联系方式：</th>
                        <td class="formValue">
                            <input id="F_Phone" name="F_Phone" type="text" class="form-control required" placeholder="联系方式" />
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <style>
        </style>
    </div>
    <script type="text/javascript">

        var keyValue = $.request("keyValue");
        $(function () {
            $.ajax({
                url: "/VehicleManage/Vehicle/GetFormJson",
                data: { keyValue: keyValue },
                dataType: "json",
                async: false,
                success: function (data) {
                    $("#form1").formSerialize(data);
                    $("#form1").find('.form-control,select,input').attr('readonly', 'readonly');
                    $("#form1").find('div.ckbox label').attr('for', '');
                }
            });
        });
    </script>
</form>